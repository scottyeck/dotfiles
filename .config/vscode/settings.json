{
  "editor.accessibilitySupport": "off",
  "window.commandCenter": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.fontLigatures": false,
  "editor.fontFamily": "CaskaydiaCove Nerd Font Mono Light",
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font Mono Light",
  "debug.console.fontFamily": "\"CaskaydiaCove Nerd Font Mono\"",
  "explorer.openEditors.visible": 0,
  "editor.fontSize": 11,
  "terminal.integrated.fontSize": 11,
  "debug.console.fontSize": 11,
  "debug.console.internalConsoleOptions": "openOnSessionStart",
  "editor.lineHeight": 16,
  "terminal.integrated.lineHeight": 1.3,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "terminal.integrated.scrollback": 100000,
  "workbench.colorTheme": "Night Owl",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.largeFileOptimizations": false,
  "redhat.telemetry.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "git.openDiffOnClick": false,
  "git.showCommitInput": false,
  "git.showActionButton": {
    "commit": false,
    "publish": false,
    "sync": false
  },
  "terminal.integrated.shellArgs.osx": ["-l"],
  "breadcrumbs.enabled": true,
  "breadcrumbs.showVariables": false,
  "breadcrumbs.showTypeParameters": false,
  "breadcrumbs.showStructs": false,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showProperties": false,
  "breadcrumbs.showPackages": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.showObjects": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showNull": false,
  "breadcrumbs.showNamespaces": false,
  "breadcrumbs.showModules": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showInterfaces": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showEnums": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showConstructors": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.icons": false,
  "breadcrumbs.symbolPath": "off",
  "vim.leader": "<space>",
  "vim.overrideCopy": true,
  "vim.ignorecase": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["K"],
      "commands": [
        {
          "when": "editorTextFocus && !editorReadOnly",
          "command": "editor.action.moveLinesUpAction"
        }
      ]
    },
    {
      "before": ["J"],
      "commands": [
        {
          "when": "editorTextFocus && !editorReadOnly",
          "command": "editor.action.moveLinesDownAction"
        }
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "after": [],
      "commands": [
        {
          "command": "undo",
          "args": []
        }
      ]
    },
    {
      "before": ["<C-r>"],
      "after": [],
      "commands": [
        {
          "command": "redo",
          "args": []
        }
      ]
    },
    {
      "before": ["-"],
      "commands": [
        {
          "command": "breadcrumbs.toggleToOn",
          "when": "!config.breadcrumbs.enabled"
        },
        {
          "command": "breadcrumbs.focusAndSelect",
          "when": "breadcrumbsPossible"
        }
      ]
    },
    {
      "before": ["<leader>", "v", "%"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "newFile",
          "when": "editorFocus"
        }
      ]
    },
    {
      "before": ["<leader>", "v", "d"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "newDir",
          "when": "editorFocus"
        }
      ]
    },
    {
      "before": ["<leader>", "v", "x"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "deleteFile",
          "when": "editorFocus"
        }
      ]
    },
    {
      "before": ["<leader>", "v", "X"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "deleteFileForce",
          "when": "editorFocus"
        }
      ]
    },
    {
      "before": ["<leader>", "v", "r"],
      "commands": [
        {
          "command": "renameFile",
          "args": "renameFile",
          "when": "editorFocus"
        }
      ]
    },
    {
      "before": [":", "e", "<CR>"],
      "commands": ["workbench.action.files.revert"]
    },
    {
      "before": ["]", "a"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "a"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "c"],
      "commands": ["workbench.action.editor.nextChange"],
      "when": "editorTextFocus && !textCompareEditorActive"
    },
    {
      "before": ["[", "c"],
      "commands": ["workbench.action.editor.previousChange"],
      "when": "editorTextFocus && !textCompareEditorActive"
    },
    {
      "before": ["<leader>", "c", "i"],
      "commands": ["merge-conflict.accept.incoming"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": ["merge-conflict.accept.current"]
    },
    {
      "before": ["<leader>", "c", "b"],
      "commands": ["merge-conflict.accept.both"]
    },
    {
      "before": ["K"],
      "commands": [
        {
          "command": "editor.action.showHover",
          "when": "editorTextFocus"
        }
      ]
    },
    {
      "before": ["g", "r"],
      "commands": [
        {
          "command": "editor.action.rename",
          "when": "editorHasRenameProvider && editorTextFocus && !editorReadOnly"
        }
      ]
    },
    {
      "before": ["g", "R"],
      "commands": [
        {
          "command": "references-view.findReferences",
          "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
        }
      ]
    },
    {
      "before": ["g", "a"],
      "commands": [
        {
          "command": "editor.action.quickFix",
          "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadOnly"
        }
      ]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "f", "t"],
      "commands": ["workbench.action.tasks.runTask"]
    },
    {
      "before": ["<leader>", "f", "c"],
      "commands": ["command-runner.run"]
    },
    {
      "before": ["<leader>", "f", "g"],
      "commands": ["find-it-faster.findWithinFiles"]
    },
    {
      "before": ["<leader>", "f", "p"],
      "commands": ["projectManager.listProjectsNewWindow"]
    },
    {
      "before": ["<leader>", "%"],
      "commands": [
        {
          "command": "editor.emmet.action.matchTag"
        }
      ]
    },
    {
      "before": ["c", "r", "c"],
      "commands": ["editor.action.transformToCamelcase"]
    },
    {
      "before": ["c", "r", "s"],
      "commands": ["editor.action.transformToSnakecase"]
    },
    {
      "before": ["c", "r", "u"],
      "commands": [
        "editor.action.transformToSnakecase",
        "editor.action.transformToUppercase"
      ]
    },
    {
      "before": ["c", "r", "-"],
      "commands": ["editor.action.transformToKebabcase"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": [
        "workbench.action.joinAllGroups",
        "workbench.action.closePanel",
        "workbench.action.closeSidebar",
        "workbench.action.closeAuxiliaryBar",
        "composer.closeComposerTab"
      ]
    },
    {
      "before": ["<leader>", "i"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["revealInExplorer"]
    },
    {
      "before": ["<leader>", "g", "y"],
      "commands": ["gitlens.copyRemoteFileUrlToClipboard"]
    },
    {
      "before": ["<leader>", "c", "i"],
      "commands": ["merge-conflict.accept.incoming"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": ["merge-conflict.accept.current"]
    },
    {
      "before": ["<leader>", "S"],
      "commands": [
        {
          "command": "search.action.openEditor",
          "when": "!inSearchEditor"
        },
        {
          "command": "search.action.focusQueryEditorWidget",
          "when": "inSearchEditor"
        }
      ]
    },
    {
      "before": ["<leader>", ":"],
      "commands": [
        {
          "command": "workbench.action.terminal.focus"
        }
      ]
    },
    {
      "before": ["<leader>", "G"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "fugitive"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": ["git.checkout"]
    },
    {
      "before": ["<leader>", "g", "c"],
      "commands": ["git.branch"]
    }
  ],
  "githubPullRequests.queries": [
    {
      "label": "Local Pull Request Branches",
      "query": "repo:${owner}/${repository} default"
    },
    {
      "label": "My Active Pull Requests",
      "query": "repo:${owner}/${repository} is:open assignee:${user}"
    },
    {
      "label": "Waiting For My Review",
      "query": "repo:${owner}/${repository} is:open review-requested:${user}"
    },
    {
      "label": "Assigned To Me",
      "query": "repo:${owner}/${repository} is:open assignee:${user}"
    },
    {
      "label": "Created By Me",
      "query": "repo:${owner}/${repository} is:open author:${user}"
    },
    {
      "label": "Mentioned Me",
      "query": "repo:${owner}/${repository} is:open mentions:${user}"
    },
    {
      "label": "All Open",
      "query": "repo:${owner}/${repository} default"
    }
  ],
  "command-runner.terminal.name": "runCommand",
  "command-runner.terminal.autoClear": true,
  "command-runner.commands": {
    "echo workspaceFolder": "echo ${workspaceFolder}",
    "echo file": "echo ${file}",
    "gwip": "git add -A; git rm $(git ls-files --deleted) 2> /dev/null; git commit --no-verify --no-gpg-sign --message \"--wip-- [skip ci]\""
  },
  "githubPullRequests.pullBranch": "never",
  "peacock.favoriteColors": [
    {
      "name": "Just Gray",
      "value": "#444"
    }
  ],
  "vscode_custom_css.imports": [
    "file:///Users/scottyeck/.config/vscode/custom.css",
    "file:///Users/scottyeck/.config/vscode/quickopen.css"
  ],
  "workbench.editor.labelFormat": "medium",
  "foam.openDailyNote.directory": "/Users/scottyeck/Dropbox/notes/daily",
  "foam.openDailyNote.filenameFormat": "yyyy-mm-dd",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.codeActionsOnSave": []
  },
  "diffEditor.renderSideBySide": true,
  "workbench.activityBar.orientation": "vertical",
  "zenMode.showTabs": "none",
  "zenMode.restore": false,
  "zenMode.hideStatusBar": false,
  "zenMode.fullScreen": false,
  "zenMode.centerLayout": true,
  "zenMode.hideLineNumbers": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "markdown.updateLinksOnFileMove.enabled": "always",
  "debug.hideLauncherWhileDebugging": true,
  "debug.toolBarLocation": "docked",
  "editor.stickyScroll.enabled": false,
  "workbench.startupEditor": "none",
  "prisma.showPrismaDataPlatformNotification": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "datadog.staticAnalysis.setup.onboarding.excludedLanguages": ["TYPESCRIPT"],
  "datadog.telemetry.setup.enableTelemetry": "disabled",
  "datadog.staticAnalysis.notification.suppressOnboarding": true,
  "workbench.editor.showTabs": "none",
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "extensions.ignoreRecommendations": true,
  "workbench.panel.defaultLocation": "right",
  "scm.diffDecorationsGutterPattern": {
    "added": true
  },
  "editor.lineNumbers": "relative",
  "debug.console.acceptSuggestionOnEnter": "on",
  "cursor.cpp.disabledLanguages": [
    "plaintext",
    "markdown",
    "scminput"
  ],
  "workbench.activityBar.location": "hidden",
  "github.copilot.nextEditSuggestions.enabled": true,
  "remote.autoForwardPortsSource": "hybrid",
  "cursor.windowSwitcher.sidebarHoverCollapsed": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "foam.logging.level": "debug",
  "terminal.integrated.tabs.enabled": false,
  "typescript.tsserver.log": "off",
  "multiDiffEditor.experimental.enabled": true
}
